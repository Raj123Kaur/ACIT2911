<body>
{{> fakeNavBar }}
{{> realNavBar }}
{{> signInUpModals }}

<div class="main-img">
  <h1 class="main-title mb-0 pt-5">Electronics</h1>
</div>
<div class="container">
  <div class="row justify-content-center">
      {{# each products}}
    <div class="product-card" style="width: 16rem; position: relative;" id="{{this.id}}">
      <div class="product-img">
          {{{this.img}}}
      </div>
      <div class="card-body align-items-center text-center">
        <h5 class="card-title text-center">{{this.title}}</h5>
        <p class="card-text">{{this.description}}</p>
        <div class="cart-info">
          <h3>${{this.price}}</h3>
            <h6>Points Cost: {{this.points}}</h6>
          <a href="/add_cart/{{this.id}}" class="btn btn-pink">Add to Cart</a>
        </div>
      </div>
    </div>
      {{/each}}
  </div>
    {{{popup}}}
</div>

{{> footer}}
